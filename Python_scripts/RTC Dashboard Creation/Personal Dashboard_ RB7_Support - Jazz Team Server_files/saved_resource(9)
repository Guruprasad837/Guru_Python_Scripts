/* "wrapper" is the box that's shown around the caret when a resource is dirty. */
.jazz-ui-MenuPopup .caret-wrapper {
	display: inline-block;
	margin-left: 2px;
	margin-right: -2px;
	vertical-align: middle;
	position: relative;
	top: -1px;

	width: 14px;
	height: 11px;
	background-color: #565656;
	border-radius: 2px;
	padding: 3px 2px;
	box-sizing: border-box;

	/* we never want the dirty wrapper to be dimmed, so make sure it's opaque. This uses
	!important since the rules that make it transparent are very nested and hard to
	overcome without unreadably long selectors.*/
	opacity: 1 !important;
}

